{
  "name": "TODO",
  "description": "TODO",
  "version": "0.0.1",
  "scripts": {
    "sapper": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "reload": "touch src/routes/_layout.svelte",
    "openbrowser": "open http://localhost:3000",
    "scss": "node-sass -x -r --output-style compressed --output ./static/ ./static/",
    "autoprefixer": "postcss --use autoprefixer --replace ./static/global.css --no-map",
    "build:css": "npm run scss --silent && npm run autoprefixer --silent && npm run reload --silent",
    "watch:css": "onchange './static/**/*.scss' -- npm run build:css --silent",
    "dev": "npm-run-all --parallel 'sapper' 'watch:css --silent' 'openbrowser'"
  },
  "dependencies": {
    "@creativelittledots/reboot": "^1.4.0",
    "compression": "^1.7.1",
    "polka": "^0.5.0",
    "sirv": "^0.4.0",
    "svelte-transitions": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "9.5.1",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "onchange": "6.0.0",
    "postcss": "7.0.16",
    "postcss-cli": "^6.1.2",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "2.9.0"
  }
}
